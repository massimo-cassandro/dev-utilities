{
  "name": "@massimo-cassandro/dev-utilities",
  "version": "1.12.5",
  "description": "My collection of dev utilities",
  "bin": {
    "update-version": "./scripts/update-version/index.mjs",
    "upd@m": "./scripts/upd@m/index.mjs",
    "vscode-css-properties-list": "./scripts/vscode-css-properties-list/index.mjs"
  },
  "type": "module",
  "scripts": {
    "update-version": "node ./scripts/update-version/index.mjs",
    "upd@m": "node ./scripts/upd@m/index.mjs",
    "npm-publish": "npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "node",
    "dev tools"
  ],
  "author": "Massimo Cassandro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/dev-utilities.git#main"
  },
  "bugs": {
    "url": "https://github.com/massimo-cassandro/dev-utilities/issues"
  },
  "homepage": "https://github.com/massimo-cassandro/dev-utilities/tree/main#readme",
  "eslintConfig": {
    "extends": [
      "./node_modules/@massimo-cassandro/linters-config/eslintrc.js"
    ]
  },
  "stylelint": {
    "extends": "@massimo-cassandro/linters-config/stylelintrc.js"
  },
  "files": [
    "scripts/**/*"
  ],
  "engines": {
    "node": ">= 16.17.0",
    "npm": ">= 9.6.0"
  },
  "dependencies": {
    "chalk": "^5.1.2",
    "clipboardy": "^3.0.0",
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@massimo-cassandro/linters-config": "^1.3.2"
  }
}